// Предположим, что у нас есть следующий объединенный массив
let combinedArray = ['Яблоко', 'Банан', 'Апельсин', 'Томат', 'Огурец', 'Перец'];

// Удаляем последний элемент массива и сохраняем его в переменную
let lastItem = combinedArray.pop();

// Выводим значение удаленного элемента на экран
console.log(lastItem);
